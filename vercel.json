{
    "version":2,
    "builds":[
        {
            "src":"./src/app.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/api(.routerAuth,routerProducts,paymentRoutes*)",
            "dest":"./src/app.js"
        },
        {
            "src":"/api/(.*)",
            "dest":"./src/app.js"
        },
        {
            "src":"/(.*)",
            "dest":"./src/app.js"
        }
    ]
}